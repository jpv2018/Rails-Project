<h1>Contest Judges:</h1>

<table border="1">
<tr>
   <td>Judge Number</td>
   <td>First Name</td>
   <td>Last Name</td>
   <td>Organization</td>
   </tr>
<% @judges.each do |p| %>
   <tr>
      <td><%= p.id %></td>
      <td><%= p.first_name %></td>
      <td><%= p.last_name %></td>
      <td><%= p.organization %></td>
      </tr>
      <% end %>
   </table>

<%= button_to "Home", '/welcome', method: :get %>

<% if current_user %>
<%= button_to "Profile", judge_path(current_user), method: :get %>
<% end %>