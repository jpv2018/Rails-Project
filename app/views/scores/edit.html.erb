<h1>Edit Score</h1>

<%= form_for(@score) do |f| %>

    <%= f.hidden_field :judge_id, value: current_user.id %>
    <%= f.hidden_field :player_id, value: @player.id %>
    <%= f.label :amount %>
    <%= f.text_field :amount %>
    <%= f.submit %>

    <%end%>

<%= button_to "Delete", score_path(@score), method: :delete %>